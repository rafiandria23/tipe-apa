{
  "name": "tipe-apa",
  "version": "0.3.3",
  "description": "Simple JavaScript data type checker.",
  "main": "./dist/index.js",
  "types": "./src/index.d.ts",
  "scripts": {
    "dev": "NODE_ENV=development tsc -p tsconfig.json --watch",
    "clean": "rm -rf ./dist",
    "build": "yarn clean && tsc -p tsconfig.json",
    "test": "yarn clean && yarn build && jest --coverage && coveralls < coverage/lcov.info",
    "prepublish:patch": "yarn build && yarn version --patch",
    "prepublish:minor": "yarn build && yarn version --minor",
    "prepublish:major": "yarn build && yarn version --major",
    "fix:link": "tsc && yarn link",
    "fix:unlink": "yarn unlink && tsc && yarn link"
  },
  "repository": "https://github.com/rafiandria23/tipe-apa.git",
  "author": "Adam Rafiandri",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^27.5.1",
    "coveralls": "^3.1.1",
    "jest": "^28.1.0",
    "ts-jest": "^28.0.2",
    "typescript": "^4.6.4"
  },
  "dependencies": {
    "@types/module-alias": "^2.0.1",
    "@types/node": "^17.0.34",
    "tslib": "^2.4.0"
  }
}
